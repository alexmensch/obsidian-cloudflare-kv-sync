{
  "name": "obsidian-cloudflare-kv-sync",
  "version": "0.1.0",
  "description": "Obsidian plugin to automatically sync Markdown files to Cloudflare KV storage.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "cloudflare",
    "kv",
    "sync",
    "markdown"
  ],
  "author": "Alex Marshall",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "8.39.1",
    "builtin-modules": "5.0.0",
    "esbuild": "0.25.9",
    "obsidian": "^1.8.7",
    "tslib": "2.8.1",
    "typescript": "5.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexmensch/obsidian-cloudflare-kv-sync.git"
  },
  "bugs": {
    "url": "https://github.com/alexmensch/obsidian-cloudflare-kv-sync/issues"
  },
  "homepage": "https://github.com/alexmensch/obsidian-cloudflare-kv-sync#readme"
}