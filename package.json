{
  "name": "obsidian-cloudflare-kv-sync",
  "version": "0.6.2",
  "description": "Obsidian plugin to automatically sync Markdown files to Cloudflare KV storage.",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && node scripts/build-assets.mjs",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "format": "npx prettier . -wl",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "config": {
    "outdir": "dist"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "cloudflare",
    "kv",
    "sync",
    "markdown"
  ],
  "author": "Alex Marshall",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.10",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "globals": "^17.1.0",
    "jest": "^30.2.0",
    "jest-environment-obsidian": "^0.0.1",
    "obsidian": "^1.11.4",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexmensch/obsidian-cloudflare-kv-sync.git"
  },
  "bugs": {
    "url": "https://github.com/alexmensch/obsidian-cloudflare-kv-sync/issues"
  },
  "homepage": "https://github.com/alexmensch/obsidian-cloudflare-kv-sync#readme"
}
