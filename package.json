{
  "name": "obsidian-cloudflare-kv-sync",
  "version": "0.1.0",
  "description": "Obsidian plugin to automatically sync Markdown files to Cloudflare KV storage.",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "format": "npx prettier . -wl",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "cloudflare",
    "kv",
    "sync",
    "markdown"
  ],
  "author": "Alex Marshall",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.3.0",
    "builtin-modules": "5.0.0",
    "esbuild": "0.25.9",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "obsidian": "latest",
    "prettier": "^3.6.2",
    "tslib": "2.8.1",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.45.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexmensch/obsidian-cloudflare-kv-sync.git"
  },
  "bugs": {
    "url": "https://github.com/alexmensch/obsidian-cloudflare-kv-sync/issues"
  },
  "homepage": "https://github.com/alexmensch/obsidian-cloudflare-kv-sync#readme"
}
